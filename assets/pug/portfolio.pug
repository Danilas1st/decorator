extends _layout

block vars
	- array = {body_class:'', layout_mod:'', sub_keywords:'', title_inner:'HOME'}
block content_block
	section.portfolio 
		.section_in 
			.category_breadcrumbs
				ul 
					li 
						a(href="#") Главная
					li 
						p Портфолио
			.portfolio_main 
				.portfolio_head 
					h1 Портфолио
				.portfolio_list 
					ul 
						-for(i = 0; i < 9; i++)
							li 
								a(href="#")
									.portfolio_img 
										img(src="i/portfolio/portfolio.png", alt="")
									.portfolio_info_who 
										p Частный заказчик
									.portfolio_info_addres
										p Темрюк, Краснодарский край
				.portfolio_pagination
					+pagination({
						pages:9,
						arrows:true
					})
