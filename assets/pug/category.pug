extends _layout

block vars
	- array = {body_class:'', layout_mod:'', sub_keywords:'', title_inner:'HOME'}
block content_block
	section.category-main 
		.section_in
			.category_breadcrumbs
				ul 
					li 
						a(href="#") Главная
					li 
						a(href="#") Униформа
					li 
						p Фартуки с грудкой
			.category-main_main
				.category-main_head 
					h1 ФАРТУКИ С ГРУДКОЙ
				.category-main_list 
					ul 
						-for(i = 0; i < 16; i++) 
							li 
								.popular_slide_img
									img(src="i/popular/popular1.png", alt="")
								.popular_price 
									span 3500 ₽
								.popular_txt 
									p Фартук женский с грудкой “Грация”  
								.popular_btn 
									a(href="#") В корзину
			.category_pagination
				+pagination({
					pages:9,
					arrows:true
				})
